pipeline {
  agent any
  triggers{
    githubPush()
  }
  stages {
    stage('Docker Build'){
      steps {
        bat 'docker build -t morawaliyadda/farmfresh-server .'
      }
    }
    stage('Docker Run'){
      steps{
        bat 'docker run -d -p 3001:3001 morawaliyadda/farmfresh-server'
      }
    }
    stage('Final'){
      steps{
        bat 'docker ps'
      }
    }}
}